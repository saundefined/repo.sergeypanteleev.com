{
    "name": "saundefined/repo.sergeypanteleev.com",
    "description": "Simple Repository Generator",
    "homepage": "https://repo.sergeypanteleev.com",
    "type": "project",
    "license": "MIT",
    "bin": [
        "bin/satis"
    ],
    "replace": {
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*"
    },
    "require": {
        "php": "^7.4||^8.0",
        "composer/composer": "^2.1.9, < 2.3",
        "symfony/console": "^4.4||^5.0",
        "twig/twig": "^3.3",
        "twig/html-extra": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Composer\\Satis\\": "src"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "ext-json": "*",
        "mikey179/vfsstream": "^1.6.8",
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Composer\\Satis\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.0-dev"
        }
    },
    "config": {
        "platform": {
            "php": "7.4.28"
        }
    }
}
